{
    "builds": [
      {
        "src": "littlelemon/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb",
          "runtime": "python3.10",
          "buildCommand": "bash build_files.sh"
         }  
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "littlelemon/wsgi.py"
      }
    ]
  }
  